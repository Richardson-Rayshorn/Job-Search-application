    <h3>Jobs below</h3>
    <div class="row">
        <% for (var i =0; i < results.length; i++) { %>
                <div class="col s12 m12 l12">
                    <div class="card">
                        <form method="" action="">
                            <h4 class="card-title"><%= results[i].title %></h4>
                            <p class="card-content"><%= results[i].descriptions %></p>
                            <p class="card-content">$<%= results[i].salary %> per month</p>
                            <input id="jobid" type="hidden" name="jobid" value="<%= results[i].id %>"/>
                            <button type="submit" class="btn">Accept Job</button>
                        </form>
                    </div>
                </div>
        <% } %>
    </div>